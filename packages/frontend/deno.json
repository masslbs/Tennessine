{
  "tasks": {
    "dev": "deno run -A ./scripts/dev.ts",
    "build": "deno run -A ./scripts/build.ts",
    "preview": "deno run -A npm:vite preview",
    "serve": "deno run --allow-net --allow-read jsr:@std/http@1/file-server dist/",
    "update-release-info": "deno run -A scripts/write-current-release.ts",
    "generate": "deno run -A scripts/generate-entry-points.ts",
    "production": "deno task update-release-info && MODE=production deno task build && deno task generate"
  },
  "exclude": ["./.vite/", "./dist/"],
  "lint": {
    "exclude": ["./src/routeTree.gen.ts"]
  },
  "fmt": {
    "exclude": ["./src/routeTree.gen.ts"]
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.4",
    "@happy-dom/global-registrator": "npm:@happy-dom/global-registrator@^17.4.4",
    "@rainbow-me/rainbowkit": "npm:@rainbow-me/rainbowkit@^2.2.0",
    "@sentry/browser": "npm:@sentry/browser@^9.13.0",
    "@std/assert": "jsr:@std/assert@^1.0.12",
    "@std/expect": "jsr:@std/expect@^1.0.15",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.4",
    "@tanstack/react-query": "npm:@tanstack/react-query@^5.74.4",
    "@tanstack/react-router": "npm:@tanstack/react-router@^1.116.0",
    "@tanstack/router-devtools": "npm:@tanstack/router-devtools@^1.116.0",
    "@tanstack/router-plugin": "npm:@tanstack/router-plugin@^1.116.1",
    "@testing-library/react": "npm:@testing-library/react@^16.3.0",
    "@testing-library/user-event": "npm:@testing-library/user-event@^14.6.1",
    "@types/node": "npm:@types/node@^22.14.1",
    "@testing-library/react-hooks": "npm:@testing-library/react-hooks@^8.0.1",
    "@types/react": "npm:@types/react@^19.1.2",
    "@types/react-dom": "npm:@types/react-dom@^19.1.2",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.4.1",
    "@vitejs/plugin-react-swc": "npm:@vitejs/plugin-react-swc@^3.9.0",
    "@wagmi/core": "npm:@wagmi/core@^2.16.3",
    "bootstrap": "npm:bootstrap@^5.3.5",
    "browser-level": "npm:browser-level@^3.0.0",
    "postcss": "npm:postcss@^8.4.49",
    "qrcode": "npm:qrcode@^1.5.4",
    "rolldown-vite": "npm:rolldown-vite@^6.3.5",
    "tailwindcss": "npm:tailwindcss@^4.1.4",
    "viem": "npm:viem@^2.27.2",
    "react": "npm:react@^19.1.0",
    "react-dom": "npm:react-dom@^19.1.0",
    "vite-plugin-node-polyfills": "npm:vite-plugin-node-polyfills@^0.23.0",
    "wagmi": "npm:wagmi@^2.14.16",
    "@uidotdev/usehooks": "npm:@uidotdev/usehooks@^2.4.1"
  }
}
