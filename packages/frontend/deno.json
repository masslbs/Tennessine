{
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read jsr:@std/http@1/file-server dist/",
    "generate": "deno run -A scripts/generate-entry-points.ts",
    "production": "MODE=production deno task build && deno task generate"
  },
  "exclude": ["./.vite/", "./dist/"],
  "lint": {
    "exclude": ["./src/routeTree.gen.ts"]
  },
  "fmt": {
    "exclude": ["./src/routeTree.gen.ts"]
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.4",
    "@happy-dom/global-registrator": "npm:@happy-dom/global-registrator@^17.4.4",
    "@rainbow-me/rainbowkit": "npm:@rainbow-me/rainbowkit@^2.2.0",
    "@std/assert": "jsr:@std/assert@^1.0.11",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.0.15",
    "@tanstack/react-query": "npm:@tanstack/react-query@^5.60.6",
    "@tanstack/react-router": "npm:@tanstack/react-router@^1.82.1",
    "@tanstack/router-devtools": "npm:@tanstack/router-devtools@^1.82.1",
    "@tanstack/router-plugin": "npm:@tanstack/router-plugin@^1.81.9",
    "@testing-library/react": "npm:@testing-library/react@^16.2.0",
    "@testing-library/user-event": "npm:@testing-library/user-event@^14.6.1",
    "@types/node": "npm:@types/node@^22.9.3",
    "@testing-library/react-hooks": "npm:@testing-library/react-hooks@^8.0.1",
    "@types/react": "npm:@types/react@^19.0.12",
    "@types/react-dom": "npm:@types/react-dom@^19.0.4",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.3",
    "@wagmi/core": "npm:@wagmi/core@^2.16.3",
    "autoprefixer": "npm:autoprefixer@^10.4.20",
    "bootstrap": "npm:bootstrap@^5.3.3",
    "browser-level": "npm:browser-level@^2.0.0",
    "postcss": "npm:postcss@^8.4.49",
    "qrcode": "npm:qrcode@^1.5.4",
    "tailwindcss": "npm:tailwindcss@^4.0.14",
    "viem": "npm:viem@2.23.12",
    "react": "npm:react@^19.0.0",
    "react-dom": "npm:react-dom@^19.0.0",
    "vite": "npm:vite@^6.2.2",
    "vite-plugin-node-polyfills": "npm:vite-plugin-node-polyfills@^0.23.0",
    "wagmi": "npm:wagmi@^2.14.12",
    "@uidotdev/usehooks": "npm:@uidotdev/usehooks@^2.4.1"
  }
}
